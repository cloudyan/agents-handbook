{
  "name": "langchain-typescript",
  "version": "0.1.0",
  "description": "LangChain TypeScript",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "check": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write src",
    "check-env": "tsx src/check-env.ts",
    "01-hello-chain": "tsx src/01-hello-chain.ts",
    "02-prompt-template": "tsx src/02-prompt-template.ts",
    "03-memory-chat": "tsx src/03-memory-chat.ts",
    "04-rag-qa": "tsx src/04-rag-qa.ts",
    "05-agent-weather": "tsx src/05-agent-weather.ts",
    "06-api-deployment": "tsx src/06-api-deployment.ts",
    "07-advanced-agents": "tsx src/07-advanced-agents.ts",
    "08-structured-output": "tsx src/08-structured-output.ts",
    "09-multi-agent": "tsx src/09-multi-agent.ts",
    "10-streaming-chat": "tsx src/10-streaming-chat.ts",
    "11-production-tracing": "tsx src/11-production-tracing.ts"
